#using UserToUI.*
#using AttrCondDefLibrary.*

#rule R21DrivingMode #with UserToUI

#source { 
	c:Context{
		-envirocontext->e
	}
	e:Environment{
		++ -activity->ac
	}
	++ ac:Activity{
		activityEnum := enum:: ActivityEnum::Driving
	}
}

#target {
	a:App{
		-navigation->nav
	}
	nav:Navigation{
		DrivingMode == true
		-menu->m
	}
	m:GridNav
}

#correspondence {
	c2a:ContextToApp{
		#src->c
		#trg->a
	}
	e2nav:EnviroToNavigation{
		#src->e
		#trg->nav
	}
}

#attributeConditions {
	
}
