#using UserToUI.*
#using AttrCondDefLibrary.*

#rule R14MiddleAgedUser #with UserToUI

#source { 
	c:Context{
		++ -usercontext->u
	}
	++ u:MiddleAgeUser{
		Age >= 45
		Age <= 60
	}
}

#target {
	a:App{
		-layout->l
	}
	l:Layout{
		++ -fontSize->fs
	}
	++fs:MiddleSize
}

#correspondence {
	c2a:ContextToApp{
		#src->c
		#trg->a
	}
	++ u2l:UserToLayout{
		#src->u
		#trg->l
	}
}

#attributeConditions {
	
}
