#using UserToUI.*
#using AttrCondDefLibrary.*

#rule R16YoungerUser #with UserToUI

#source { 
	c:Context{
		++ -usercontext->u
	}
	++ u:YoungerUser{
		Age <= 45
	}	
}

#target {
	a:App{
		-layout->l
	}
	l:Layout{
		++ -fontSize->fs
	}
	++fs:DefaultSize
}

#correspondence {
	c2a:ContextToApp{
		#src->c
		#trg->a
	}
	++ u2l:YUserToLayout{
		#src->u
		#trg->l
	}
}

#attributeConditions {
	
}
